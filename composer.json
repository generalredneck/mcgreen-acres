{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.27",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.0",
        "drupal/ctools": "^3.2",
        "drupal/devel_entity_updates": "^1.0@RC",
        "drupal/diff": "^1.0@RC",
        "drupal/disqus": "^1.0@RC",
        "drupal/evercurrent": "^2.0",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/realname": "^1.0@RC",
        "drupal/redirect": "^1.4",
        "drupal/scheduler": "^1.1",
        "drupal/search_api": "^1.14",
        "drupal/seo_checklist": "^4.1",
        "drupal/simple_block": "^1.0@beta",
        "drupal/twig_tweak": "^2.4",
        "drupal/varbase_bootstrap_paragraphs": "7.1",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.0.0",
        "npm-asset/slick-carousel": "^1.8",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/stage_file_proxy": "^1.0@beta",
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0.33"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "build-assets": [
            "echo 'Nothing to build'"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "config": {
            "optimize-autoloader": true,
            "sort-packages": true
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/varbase_bootstrap_paragraphs": {
                "bootstrap4": "patches/varbase_bootstra_paragraphs-bootstrap4.patch"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
