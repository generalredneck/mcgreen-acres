{
    "name": "mcgreenacres/mcgreenacres-com",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mozilla/pdf.js",
                "version": "5.1.91",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/mozilla/pdf.js/releases/download/v5.1.91/pdfjs-5.1.91-dist.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "5.9.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dropzone/dropzone/releases/download/v5.9.3/dist.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://git.drupalcode.org/issue/commerce_timeslots-3401757.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^1.0 || ^2.3",
        "cweagans/composer-patches": "^1.7.3",
        "drupal/admin_toolbar": "^3.4.2",
        "drupal/antibot": "^2.0",
        "drupal/block_class": "^4.0",
        "drupal/block_visibility_groups": "^2.0.1",
        "drupal/commerce_fee": "^1.0@dev",
        "drupal/commerce_prorater_stepped_proportional": " ^2.0",
        "drupal/commerce_recipe_core": "^1.0",
        "drupal/commerce_recurring": "^1.0@RC",
        "drupal/commerce_stock": "^3.0@beta",
        "drupal/commerce_stripe": "^2.0",
        "drupal/commerce_timeslots": "dev-3401757-date-picker-upgrade",
        "drupal/commerce_variation_bundle": "^1.0",
        "drupal/config_split": "^2.0",
        "drupal/core": "^11.0",
        "drupal/core-composer-scaffold": "^11.0",
        "drupal/core-vendor-hardening": "^11.2",
        "drupal/ctools": "^4.1",
        "drupal/default_content": "^2.0@alpha",
        "drupal/diff": "^1.7@RC",
        "drupal/disqus": "^2.0.2",
        "drupal/easy_breadcrumb": "^2.0.7",
        "drupal/entity_embed": "^1.7",
        "drupal/facets_pretty_paths": "^2.0",
        "drupal/file_delete": "^3.0",
        "drupal/google_analytics": "^4.0.2",
        "drupal/honeypot": "^2.2",
        "drupal/image_delta_formatter": "^1.3",
        "drupal/jquery_ui": "^1.7",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_slider": "^2.1",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/key": "^1.20",
        "drupal/mailsystem": "^4.5",
        "drupal/menu_block": "^1.11",
        "drupal/pathauto": "^1.12",
        "drupal/pdf": "^1.2",
        "drupal/realname": "^2.0@beta",
        "drupal/redirect": "^1.9",
        "drupal/scheduler": "^2.1",
        "drupal/schema_metatag": "^3.0.1",
        "drupal/search_api": "^1.35",
        "drupal/seo_checklist": "^5.2",
        "drupal/simple_block": "^1.6@beta",
        "drupal/symfony_mailer": "^1.5",
        "drupal/term_condition": "^2.0",
        "drupal/twig_tweak": "^3.3",
        "drupal/upgrade_status": "^4.3",
        "drupal/varbase_bootstrap_paragraphs": "^10.0.9",
        "drupal/webform": "^6.3@beta",
        "drupal/webform_ip_geo": "^1.0",
        "drupal/webform_mailchimp": "^5.11",
        "drupal/xmlsitemap": "^2.0",
        "drush/drush": "^13",
        "enyo/dropzone": "^5.9.3",
        "fourkitchens/pots": "general-deploy-dev",
        "mglaman/composer-drupal-lenient": "^1.0",
        "mozilla/pdf.js": "^5.1",
        "npm-asset/blazy": "^1.8.2",
        "oomphinc/composer-installers-extender": "^2.0.1",
        "thinkshout/mailchimp-api-php": "^3.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "platform": {
            "php": "8.4"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-vendor-hardening": true,
            "drupal/drupal-library-installer-plugin": true,
            "mglaman/composer-drupal-lenient": true,
            "oomphinc/composer-installers-extender": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/.htaccess": false
            }
        },
        "drupal-core-vendor-hardening": {
            "drupal/core": [
                "tests",
                "profiles/demo_umami"
            ],
            "drupal/commerce_stripe": [
                "tests"
            ],
            "stripe/stripe": [
                "README.md"
            ]
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "vendor:bower-asset",
                "vendor:npm-asset",
                "enyo/dropzone"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/block_visibility_groups": {
                "3503194": "https://git.drupalcode.org/project/block_visibility_groups/-/merge_requests/26.diff"
            },
            "drupal/bootstrap_basic_image_gallery": {
                "once": "patches/bootstrap_basic_image_gallery_once.patch"
            },
            "drupal/commerce_prorater_stepped_proportional": {
                "d11": "https://www.drupal.org/files/issues/2024-07-21/commerce_prorater_stepped_proportional.2.x-dev.rector.patch"
            },
            "drupal/disqus": {
                "php 8.4": "patches/disqus-3542113-2.patch"
            },
            "drupal/pdf": {
                "drupal 11": "patches/pdf-3433888-11-mr5.patch"
            },
            "drupal/varbase_media": {
                "Role upgrade fix": "patches/varbase_media_role_upgrade_100005.patch"
            },
            "drupal/webform_ip_geo": {
                "php 8.4": "patches/webform_ip_geo-3542116-3-MR5.patch",
                "drupal 11": "patches/webform_ip_geo-3435648-5-mr4.patch"
            },
            "drupal/webform_mailchimp": {
                "Add Tags": "https://www.drupal.org/files/issues/2023-12-06/add_configurable_tags-3406527-1.patch"
            }
        },
        "drupal-lenient": {
            "allowed-list": [
                "drupal/pdf",
                "drupal/commerce_prorater_stepped_proportional",
                "drupal/webform_ip_geo"
            ]
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "build-assets": "echo No assets to build",
        "lint": [
            "find web/modules/custom web/themes/custom \\( -iname  '*.php' -o -iname '*.inc' -o -iname '*.module' -o -iname '*.install'-o -iname '*.theme' \\) '!' -path '*/node_modules/*' -print0 | xargs -0 -n1 -P8 php -l"
        ],
        "code-sniff": [
            "echo this code smells"
        ]
    }
}
