{
    "name": "mcgreenacres/mcgreenacres-com",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^2.3",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^8.9.16",
        "drupal/core-recommended": "^8.9.16",
        "drupal/ctools": "^3.2",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/diff": "^1.0@RC",
        "drupal/disqus": "^1.0@RC",
        "drupal/evercurrent": "^2.0",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/realname": "^1.0@RC",
        "drupal/redirect": "^1.4",
        "drupal/scheduler": "^1.1",
        "drupal/search_api": "^1.14",
        "drupal/seo_checklist": "^4.1",
        "drupal/simple_block": "^1.0@beta",
        "drupal/twig_tweak": "^2.4",
        "drupal/varbase_bootstrap_paragraphs": "^7.1",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "enyo/dropzone": "^5.8",
        "npm-asset/blazy": "^1.6.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/stage_file_proxy": "^1.0@beta"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }

        },
        "installer-types": ["npm-asset", "bower-asset"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "vendor:bower-asset",
                "vendor:npm-asset",
                "enyo/dropzone"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/varbase_media": {
                "#3153617": "https://www.drupal.org/files/issues/2021-03-12/varbase_media-without_varbase_profile_update_entities-3153617-5.patch"
            }
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "build-assets": "echo No assets to build"
    }
}
