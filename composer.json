{
    "name": "mcgreenacres/mcgreenacres-com",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.0 || ^2.0",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^2.3",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/ctools": "^3.2",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/diff": "^1.0@RC",
        "drupal/disqus": "^1.0@RC",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.29",
        "drupal/menu_block": "^1.5",
        "drupal/metatag": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/realname": "^2.0@beta",
        "drupal/redirect": "^1.4",
        "drupal/scheduler": "^1.1",
        "drupal/schema_metatag": "^2.2",
        "drupal/search_api": "^1.14",
        "drupal/seo_checklist": "^5.0",
        "drupal/simple_block": "^1.0@beta",
        "drupal/twig_tweak": "^2.4",
        "drupal/upgrade_status": "^4.3",
        "drupal/varbase_bootstrap_paragraphs": "^9.0",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^11 || ^12 || ^13",
        "enyo/dropzone": "^5.8",
        "fourkitchens/pots": "general-deploy-dev",
        "npm-asset/blazy": "^1.6.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0"
    },
    "require-dev": {
        "drupal/stage_file_proxy": "^1.0@beta"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true,
            "drupal/console-extend-plugin": true,
            "drupal/drupal-library-installer-plugin": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }

        },
        "installer-types": ["npm-asset", "bower-asset"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "vendor:bower-asset",
                "vendor:npm-asset",
                "enyo/dropzone"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "build-assets": "echo No assets to build",
        "lint": [
            "./node_modules/.bin/eslint ./",
            "find web/modules/custom web/themes/custom \\( -iname  '*.php' -o -iname '*.inc' -o -iname '*.module' -o -iname '*.install'-o -iname '*.theme' \\) '!' -path '*/node_modules/*' -print0 | xargs -0 -n1 -P8 php -l"
        ],
        "code-sniff": [
            "echo this code smells"
        ]
    }
}
